{
  "name": "react-admin-copyfield",
  "version": "1.0.0",
  "description": "A simple, reusable CopyField component for React Admin and MUI. Supports copying a value from a field with a single click and tooltip feedback.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --dts --external react,react-dom,react/jsx-runtime,@mui/material,react-admin",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "react-admin",
    "mui",
    "material-ui",
    "copy",
    "clipboard",
    "field",
    "component"
  ],
  "author": "Toan Bui <toanbui-tech>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/toanbui-tech/react-admin-copyfield.git"
  },
  "bugs": {
    "url": "https://github.com/toanbui-tech/react-admin-copyfield/issues"
  },
  "homepage": "https://github.com/toanbui-tech/react-admin-copyfield#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-admin": ">=4.0.0",
    "@mui/material": ">=5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0"
  }
}
